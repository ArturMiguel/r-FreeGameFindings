{
  "name": "reddit-scraper",
  "version": "1.0.0",
  "description": "Scraper that collects free games posts published on Reddit and sends them to Discord",
  "main": "index.js",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only --clear ./src/index.ts",
    "prestart": "npm run build",
    "start": "node dist/index.js"
  },
  "keywords": [
    "webscraping",
    "reddit",
    "discord",
    "webhook"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArturMiguel/r-FreeGameFindings.git"
  },
  "author": "Artur Miguel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArturMiguel/r-FreeGameFindings/issues"
  },
  "homepage": "https://github.com/ArturMiguel/r-FreeGameFindings#readme",
  "dependencies": {
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "moment-timezone": "^0.5.46",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/node-cron": "^3.0.11",
    "ts-node-dev": "^2.0.0"
  }
}
